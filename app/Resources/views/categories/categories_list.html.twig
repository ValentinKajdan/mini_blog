{% extends 'base.html.twig' %}

{% block body %}
  <div class="container">
    <ul>
    {% for category in categories %}
        <li>
          <a href="{{ path('categories_name', {'id': category.id}) }}">
            {{ category.title }}
          </a>
        </li>
    {% endfor %}
    </ul>
  </div>
{% endblock %}
